{
  "sea_level": 40,
  "disable_mob_generation": false,
  "aquifers_enabled": true,
  "ore_veins_enabled": true,
  "legacy_random_source": false,
  "default_block": { "Name": "minecraft:stone" },
  "default_fluid": { "Name": "minecraft:lava", "Properties": { "level": "0" } },
  "noise": { "min_y": -64, "height": 384, "size_horizontal": 1, "size_vertical": 2 },

  "noise_router": {
    "barrier": { "type": "minecraft:constant", "argument": 0.0 },
    "fluid_level_floodedness": { "type": "minecraft:constant", "argument": -0.5 },
    "fluid_level_spread": { "type": "minecraft:constant", "argument": 0.1 },
    "lava": { "type": "minecraft:constant", "argument": 0.0 },

    "temperature": {
      "type": "minecraft:shifted_noise",
      "shift_x": { "type": "minecraft:constant", "argument": 0.0 },
      "shift_y": { "type": "minecraft:constant", "argument": 0.0 },
      "shift_z": { "type": "minecraft:constant", "argument": 0.0 },
      "noise": "minecraft:temperature",
      "xz_scale": 0.25,
      "y_scale": 0.0
    },
    "vegetation": {
      "type": "minecraft:shifted_noise",
      "shift_x": { "type": "minecraft:constant", "argument": 0.0 },
      "shift_y": { "type": "minecraft:constant", "argument": 0.0 },
      "shift_z": { "type": "minecraft:constant", "argument": 0.0 },
      "noise": "minecraft:vegetation",
      "xz_scale": 0.25,
      "y_scale": 0.0
    },
    "continents": { "type": "minecraft:noise", "noise": "minecraft:continentalness", "xz_scale": 1.0, "y_scale": 0.0 },
    "erosion": { "type": "minecraft:noise", "noise": "minecraft:erosion", "xz_scale": 1.0, "y_scale": 0.0 },
    "depth": { "type": "minecraft:y_clamped_gradient", "from_y": -64, "to_y": 128, "from_value": 1.0, "to_value": -1.0 },
    "ridges": { "type": "minecraft:noise", "noise": "minecraft:erosion", "xz_scale": 2.0, "y_scale": 0.5 },

    "initial_density_without_jaggedness": {
      "type": "minecraft:add",
      "argument1": {
        "type": "minecraft:mul",
        "argument1": { "type": "minecraft:noise", "noise": "minecraft:erosion", "xz_scale": 1.0, "y_scale": 1.0 },
        "argument2": { "type": "minecraft:constant", "argument": 0.7 }
      },
      "argument2": {
        "type": "minecraft:mul",
        "argument1": { "type": "minecraft:noise", "noise": "minecraft:erosion", "xz_scale": 2.0, "y_scale": 0.5 },
        "argument2": { "type": "minecraft:constant", "argument": 0.3 }
      }
    },

    "final_density": {
      "type": "minecraft:add",
      "argument1": {
        "type": "minecraft:add",
        "argument1": { "type": "minecraft:noise", "noise": "minecraft:erosion", "xz_scale": 1.0, "y_scale": 0.25 },
        "argument2": { "type": "minecraft:constant", "argument": -0.2 }
      },
      "argument2": {
        "type": "minecraft:add",
        "argument1": { "type": "minecraft:constant", "argument": 0.1 },
        "argument2": { "type": "minecraft:y_clamped_gradient", "from_y": -64, "to_y": 192, "from_value": -0.5, "to_value": 0.8 }
      }
    },
    "vein_toggle": { "type": "minecraft:constant", "argument": 0.0 },
    "vein_ridged": { "type": "minecraft:constant", "argument": 0.0 },
    "vein_gap": { "type": "minecraft:constant", "argument": 0.0 }
  },

  "surface_rule": {
    "type": "minecraft:sequence",
    "sequence": [
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:vertical_gradient",
          "random_name": "bedrock_floor",
          "true_at_and_below": { "absolute": -64 },
          "false_at_and_above": { "absolute": -60 }
        },
        "then_run": { "type": "minecraft:block", "result_state": { "Name": "minecraft:bedrock" } }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:y_above",
          "anchor": { "above_bottom": 128 },
          "surface_depth_multiplier": 0.0,
          "add_stone_depth": false,
          "secondary_depth_range": 0
        },
        "then_run": { "type": "minecraft:block", "result_state": { "Name": "minecraft:crimson_nylium" } }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:y_above",
          "anchor": { "above_bottom": 64 },
          "surface_depth_multiplier": 0.0,
          "add_stone_depth": false,
          "secondary_depth_range": 0
        },
        "then_run": { "type": "minecraft:block", "result_state": { "Name": "minecraft:stone" } }
      },
      {
        "type": "minecraft:condition",
        "if_true": {
          "type": "minecraft:y_above",
          "anchor": { "above_bottom": 0 },
          "surface_depth_multiplier": 0.0,
          "add_stone_depth": false,
          "secondary_depth_range": 0
        },
        "then_run": { "type": "minecraft:block", "result_state": { "Name": "minecraft:deepslate" } }
      }
    ]
  },


  "spawn_target": []
}
